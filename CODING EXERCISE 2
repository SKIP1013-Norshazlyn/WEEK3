		String name = "Norshazlyn Idayu binti Zaidi";
		String pgm = "Science";
		
		double gpa1 = 3.59;
		double gpa2 = 3.60;
		double cgpa = (gpa1 + gpa2)/ 2;
		
		System.out.println("Name : " +name);
		System.out.println("Programme : " +pgm);
		System.out.println("College : Kolej Matrikulasi Pulau Pinang");
